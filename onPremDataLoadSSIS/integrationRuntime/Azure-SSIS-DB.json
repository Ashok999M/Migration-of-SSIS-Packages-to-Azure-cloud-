{
    "name": "Azure-SSIS-DB",
    "properties": {
        "type": "Managed",
        "typeProperties": {
            "computeProperties": {
                "location": "East US",
                "nodeSize": "Standard_D2_v3",
                "numberOfNodes": 1,
                "maxParallelExecutionsPerNode": 1
            },
            "ssisProperties": {
                "catalogInfo": {
                    "catalogServerEndpoint": "automate.database.windows.net",
                    "catalogAdminUserName": "Automate",
                    "catalogAdminPassword": {
                        "type": "SecureString",
                        "value": "**********"
                    },
                    "catalogPricingTier": "Basic"
                },
                "edition": "Standard",
                "licenseType": "LicenseIncluded",
                "dataProxyProperties": {
                    "connectVia": {
                        "type": "IntegrationRuntimeReference",
                        "referenceName": "self-hosted-IR"
                    },
                    "stagingLinkedService": {
                        "type": "LinkedServiceReference",
                        "referenceName": "AzureBlobStorage1"
                    }
                }
            }
        }
    }
}